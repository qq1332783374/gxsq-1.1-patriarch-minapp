<view class='myTab3'>
    
    <view class='jobinfo' wx:for='{{mytabdata}}' wx:key='index'>
    <navigator url="/pages/textWork/textWork?postUUID={{item.jobUUID}}">
    
    <view class="card">

      <view class="card-item">

        <view class="header">
          <view class="title">
            <text class="bolder">{{item.postName}}</text>
          </view>

        </view>

        <view class="content">
          <view class="desc">
            <view class="text-conetnt">
              <text>{{item.orgName}}</text>
              
            </view>
            <view class="date">
              薪资：{{item.salary}}
            </view>
          </view>
        </view>
      </view>
    </view>
    </navigator>
  </view>

</view>